{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Noisrev.League.IO.RST/Noisrev.League.IO.RST.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "namespaceLayout": "nested",
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ]
  }
}
